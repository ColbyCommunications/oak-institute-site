{
    "name": "colbycommunications/oak-institute-site",
    "description": "Configuration project for the oakinstitute.colby.edu site.",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.7.1",
        "wpackagist-plugin/elementor": "3.26.3",
        "wpackagist-plugin/header-footer": "3.3.0",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/wordpress-seo": "23.0",
        "wpackagist-plugin/cloudflare": "4.12.8",
        "wpackagist-plugin/public-post-preview": "3.0.1",
        "wpackagist-plugin/wp-saml-auth": "2.1.4",
        "wpackagist-plugin/siteimprove": "2.0.7",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/elementor-pro": "3.26.2",
        "colbycommunications/gravityforms": "2.9.1.3",
        "colbycommunications/colby-ticket": "^1.0.1",
        "wpackagist-plugin/wp-search-with-algolia": "2.8.2",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/classic-editor": "1.6.7",
        "wpackagist-plugin/wp-nested-pages": "3.2.10",
        "wpackagist-plugin/disable-comments": "2.4.6",
        "wpackagist-theme/hello-elementor": "3.2.1",
        "colbycommunications/wp-all-export-pro": "1.9.4"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        },
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-oak-institute-theme"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
