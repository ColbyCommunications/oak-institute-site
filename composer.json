{
  "name": "colby-college/some-site",
  "description": "Configuration project for the some-site.colby.edu site",
  "type": "project",
  "require": {
    "johnpbloch/wordpress-core-installer": "^2.0",
    "johnpbloch/wordpress-core": "^5",
    "wpackagist-theme/twentyseventeen": "^2",
    "wpackagist-plugin/classic-editor": "^1",
    "wpackagist-plugin/elementor": "^3",
    "colbycommunications/colby-groups": "dev-wp4-test",
    "wpackagist-plugin/capability-manager-enhanced": "^2",
    "wpackagist-plugin/tablepress": "^1.10",
    "wpackagist-theme/hello-elementor": "^2",
    "wpackagist-plugin/header-footer": "^3",
    "wpackagist-plugin/duplicate-post": "^4",
    "wpackagist-plugin/wordpress-seo": "^17",
    "wpackagist-plugin/cloudflare": "^4",
    "devgeniem/wp-redis-object-cache-dropin": "^1.6",
    "wpackagist-plugin/cache-control": "^2.2",
    "colbycommunications/colby-base-theme": "dev-master",
    "wpackagist-plugin/public-post-preview": "^2.9",
    "wp-graphql/wp-graphql": "^1.6",
    "wp-graphql/wp-graphiql": "^1.0",
    "pristas-peter/wp-graphql-gutenberg": "^0.3",
    "wpackagist-plugin/wp-saml-auth": "^2",
    "wpackagist-plugin/siteimprove": "^1",
    "wpackagist-plugin/query-monitor": "^3",
    "colbycommunications/colby-oak-institute-theme": "dev-master"
  },
  "authors": [
    {
      "name": "Morty Mule",
      "email": "webmaster@colby.edu"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ColbyCommunications/colby-base-theme"
    },
    {
      "type": "git",
      "url": "https://github.com/ColbyCommunications/colby-groups"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org/"
    },
    {
      "type": "git",
      "url": "https://github.com/ColbyCommunications/colby-oak-institute-theme"
    }
  ],
  "extra": {
    "wordpress-install-dir": "web/wp",
    "installer-paths": {
      "web/wp-content/mu-plugins/{$name}": [
        "type:wordpress-muplugin",
        "wpackagist-plugin/wordpress-seo"
      ],
      "web/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "web/wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/installers": true,
      "koodimonni/composer-dropin-installer": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
